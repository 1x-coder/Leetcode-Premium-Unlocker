{
    "name": "Leetcode Premium Data Injector",
    "version": "0.4.0",
    "description": "Inject crowdsourced Leetcode premium data",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "dist/index.html"
    },
    "manifest_version": 3, 
    "content_scripts": [
        {
          "css": ["dist/style.css"], 
          "js": ["/dist/main.js"],
          "matches": [
            "https://leetcode.com/*"
          ]
        }
      ], 
    "icons": {
      "96": "/assets/logo.png"
    }
}