{
    "name": "Leetcode Premium Unlocker",
    "version": "0.3.0",
    "description": "Inject crowdsourced Leetcode premium data",
    "permissions": [],
    "action": {
      "default_popup": "dist/index.html"
    },
    "manifest_version": 3, 
    "content_scripts": [
        {
          "js": ["/dist/main.js"],
          "matches": [
            "https://leetcode.com/*"
          ]
        }
      ], 
    "icons": {
      "96": "/assets/logo.png"
    }
}